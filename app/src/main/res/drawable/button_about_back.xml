<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" >
    <item android:state_pressed="true" >
        <shape android:shape="rectangle"  >
            <corners android:radius="22dip" />
            <stroke android:width="1dip" android:color="@color/btnAboutPressedBorderColorBack" />
            <gradient android:angle="-90" android:startColor="@color/btnAboutPressedStartColorBack" android:endColor="@color/btnAboutPressedEndColorBack"  />
        </shape>
    </item>
    <item android:state_focused="true">
        <shape android:shape="rectangle"  >
            <corners android:radius="22dip" />
            <stroke android:width="1dip" android:color="@color/btnAboutPressedBorderColorBack" />
            <solid android:color="@color/btnAboutShapeEndColorBack"/>
        </shape>
    </item>
    <item >
        <shape android:shape="rectangle"  >
            <corners android:radius="22dip" />
            <stroke android:width="1dip" android:color="@color/btnAboutShapeBorderColorBack" />
            <gradient android:angle="-90" android:startColor="@color/btnAboutShapeStartColorBack" android:endColor="@color/btnAboutShapeEndColorBack" />
        </shape>
    </item>
</selector>